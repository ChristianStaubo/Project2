<div class="container">
    <div class="row">
      <div class="col-sm-9 col-md-7 col-lg-5 mx-auto">
        <div class="card border-0 shadow rounded-3 my-5">
          <div class="card-body p-4 p-sm-5">
            <h5 class="card-title text-center mb-5 fw-light fs-5">Sign Up</h5>
            <form action="/session/register" method="POST">
              <div class="form-floating mb-3">
                <input type="username" class="form-control" id="username" name="username" placeholder="Username">
                <label for="Username">Username</label>
              </div>
              <div class="form-floating mb-3">
                <input type="password" class="form-control" id="password" name="password" placeholder="Password">
                <label for="password">Password</label>
              </div>
              <div class="form-floating mb-3">
                <input type="password" class="form-control" id="verifyPassword" name="verifyPassword" placeholder="Password">
                <label for="password">Verify Password</label>
              </div>
              <div class="d-grid">
                <input type="submit" class="btn btn-primary btn-login text-uppercase fw-bold" value="Sign up">

              </div>
            </form>
          </div>
        </div>
      </div>
    </div>
  </div>